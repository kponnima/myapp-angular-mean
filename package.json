{
  "name": "myapp",
  "version": "1.0.0",
  "engines": {
    "node": "12.3.1",
    "npm": "6.10.1"
  },
  "scripts": {
    "ng": "ng",
    "version": "ng version",
    "update": "ng update",
    "start": "nodemon ./app.js",
    "serve": "ng serve -o",
    "serve-test": "rimraf dist/* && ng build --watch --progress=true",
    "build": "ng build --prod",
    "build-test": "lite-server",
    "dev": "concurrently \"npm run mongo-win\" \"npm run start\" \"npm run serve\"",
    "pretest": "cross-env NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha -r \"test/helper.js\" --recursive \"test/server/**/*_test.js\"",
    "test": "ng test",
    "posttest": "./node_modules/.bin/gulp",
    "coverage": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "tslint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "e2e-noupdate": "ng e2e --no-webdriver-update",
    "driver-update": "webdriver-manager update",
    "driver-update-noSSL": "webdriver-manager update --ignore_ssl",
    "driver-version": "webdriver-manager status",
    "driver-clean": "webdriver-manager clean",
    "mongo-win": "md mongo-db & \"C:/Program Files/MongoDB/Server/4.2/bin/mongod.exe\" --auth --port 27017 --dbpath mongo-db",
    "mongo-win-noauth": "md mongo-db & \"C:/Program Files/MongoDB/Server/4.2/bin/mongod.exe\" --port 27017 --dbpath mongo-db"
  },
